{
  "name": "veto-sdk",
  "version": "1.0.0",
  "description": "Guardrails for AI agent tool calls - intercept, validate, and control",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "benchmark": "node dist/benchmark/cli.js",
    "benchmark:dev": "tsx src/benchmark/cli.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "guardrail",
    "tool-calls",
    "safety",
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "claude",
    "veto"
  ],
  "author": "Plaw, Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "openai": "^4.77.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "openai": "^4.0.0",
    "zod": "^3.22.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "import": "./dist/rules/index.js"
    },
    "./kernel": {
      "types": "./dist/kernel/index.d.ts",
      "import": "./dist/kernel/index.js"
    }
  },
  "dependencies": {
    "yaml": "^2.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VulnZap/veto",
    "directory": "packages/sdk"
  },
  "homepage": "https://veto.run",
  "bugs": {
    "url": "https://github.com/VulnZap/veto/issues"
  }
}
